<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/quiz_drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui8.QuizHomeActivity">

    <!-- 主内容区域 -->
    <FrameLayout
        android:id="@+id/quiz_content_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/question_detail_fragment8"
            android:name="com.yuanseen.shuati.ui8.QuestionDetailFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout="@layout/fragment_question_detail8" />

    </FrameLayout>
</androidx.drawerlayout.widget.DrawerLayout>